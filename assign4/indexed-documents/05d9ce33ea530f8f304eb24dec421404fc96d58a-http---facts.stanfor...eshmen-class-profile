<script type="text/javascript" src="/http://facts.stanford.edu/academics/../js/canvasjs.min.js"></script>
<script type="text/javascript">
  window.onload = function () {
    var gender, chartGender
      , hs,     chartHighSchools
      , ethnic, chartEthnic
      ;
// Gender Balance
    gender = {
        male:   parseFloat($('table#gender td#male').text().trim().replace('%',''))
      , female: parseFloat($('table#gender td#female').text().trim().replace('%',''))
    };
    chartGender = new CanvasJS.Chart("gender-balance-pie", {
      theme: "theme3",
      backgroundColor: "#FBFBF9",
      toolTip:{ enabled: false},
      data: [//array of dataSeries              
        { //dataSeries object

         /*** Change type "column" to "bar", "area", "line" or "pie"***/
         type: "pie",
         indexLabelFontColor: "#000",
         indexLabelFontSize: 14,
         //indexLabelPlacement: "inside",
         dataPoints: [
         { label: "Men",   y: gender.male,   indexLabel: "Men "   + gender.male.toFixed(0)  + "%" },
         { label: "Women", y: gender.female, indexLabel: "Women " + gender.female.toFixed(0) + "%" },
         ]
       }
       ]
     });
    chartGender.render();

 // High Schools
    hs = {
        public:  parseFloat($('table#high-school td#public').text().trim().replace('%',''))
      , private: parseFloat($('table#high-school td#private').text().trim().replace('%',''))
      , home:    parseFloat($('table#high-school td#home').text().trim().replace('%',''))
      , intl:    parseFloat($('table#high-school td#intl-school').text().trim().replace('%',''))
    };
    chartHighSchools = new CanvasJS.Chart("high-schools-pie", {
      theme: "theme3",
      backgroundColor: "#FBFBF9",
      toolTip:{ enabled: false},
      data: [//array of dataSeries              
        { //dataSeries object

         /*** Change type "column" to "bar", "area", "line" or "pie"***/
         type: "pie",
         indexLabelFontColor: "#000",
         indexLabelFontSize: 14,
         dataPoints: [
         { label: "Public",        y: hs.public,  indexLabel: "Public "        + hs.public.toFixed(0)  + "%" },
         { label: "Private",       y: hs.private, indexLabel: "Private "       + hs.private.toFixed(0) + "%" },
         { label: "Home school",   y: hs.home,    indexLabel: "Home school <"   + hs.home.toFixed(0)    + "%" },
         { label: "International", y: hs.intl,    indexLabel: "International " + hs.intl.toFixed(0)    + "%" },
         ]
       }
       ]
     });
    chartHighSchools.render();

  // Ethnic Diversity
    ethnic = {
        afam:     parseFloat($('table#ethnic td#afam').text().trim().replace('%',''))
      , asian:    parseFloat($('table#ethnic td#asian').text().trim().replace('%',''))
      , intl:     parseFloat($('table#ethnic td#intl').text().trim().replace('%',''))
      , native:   parseFloat($('table#ethnic td#native').text().trim().replace('%',''))
      , pacific:  parseFloat($('table#ethnic td#pacific').text().trim().replace('%',''))
      , hispanic: parseFloat($('table#ethnic td#lat').text().trim().replace('%',''))
      , white:    parseFloat($('table#ethnic td#white').text().trim().replace('%',''))
      , unknown:  parseFloat($('table#ethnic td#unknown').text().trim().replace('%',''))
      , twoormore:  parseFloat($('table#ethnic td#twoormore').text().trim().replace('%',''))
    };
    chartEthnic = new CanvasJS.Chart("ethnic-diversity-pie", {
      theme: "theme3",
      backgroundColor: "#FBFBF9",
      toolTip:{ enabled: false},
      data: [//array of dataSeries              
        { //dataSeries object

         /*** Change type "column" to "bar", "area", "line" or "pie"***/
         type: "doughnut",
         indexLabelFontColor: "#000",
         indexLabelFontFamily: "Source Sans Pro",
         indexLabelFontSize: 14,
         dataPoints: [
         { label: "Native American",                    y: ethnic.native,   indexLabel: "Native American "   + ethnic.native.toFixed(0)   + "%" },
         { label: "Native Hawaiian / Pacific Islander", y: ethnic.pacific,  indexLabel: "Native Hawaiian / Pacific Islander " + ethnic.pacific.toFixed(0) + "%" },
         { label: "Other Hispanic",                     y: ethnic.hispanic, indexLabel: "Other Hispanic "    + ethnic.hispanic.toFixed(0) + "%" },
         { label: "White",                              y: ethnic.white,    indexLabel: "White "             + ethnic.white.toFixed(0)    + "%" },
         { label: "Unknown",                            y: ethnic.unknown,  indexLabel: "Unknown "           + ethnic.unknown.toFixed(0)  + "%" },
         { label: "African American",                   y: ethnic.afam,     indexLabel: "African American "  + ethnic.afam.toFixed(0)     + "%" },
         { label: "Asian",                              y: ethnic.asian,    indexLabel: "Asian "             + ethnic.asian.toFixed(0)    + "%" },
         { label: "International",                      y: ethnic.intl,     indexLabel: "International "     + ethnic.intl.toFixed(0)     + "%" },
         { label: "Two or more races",                  y: ethnic.twoormore,     indexLabel: "Two or more races "     + ethnic.twoormore.toFixed(0)     + "%" },           
       ]
       }
       ]
     });
    chartEthnic.render();
  }
</script>
