(function($) {
    $(function() {
        $('#panel_52').magnificPopup({
            type:'inline',
            delegate: '.b-popup-trigger',
			midClick: true,
			gallery: {
				enabled: true
			},
			callbacks: {
				change: function() {
					$(".lazyload, .lazyload-embed-thumb, .intro-lazyload", this.content).each(function() {
						var $this = $(this);
						if( $this.is( 'img' ) ){
							$this.attr( 'src', $this.attr( 'data-src' ) ).css( 'opacity', 1 );
						} else {
							$this.css( 'background-image', sprintf( 'url(%s)', $this.attr( 'data-src' ) ) ).css( 'opacity', 1 );
						}
					});

					// send popup change event to Google Analytics data layer
					if (window.dataLayer) {
						var $title = $('.b-popup-title', this.content);

						window.dataLayer.push({
							'event': 'onPopupChange',
							'eventCategory': 'Popups',
							'eventAction': 'Promos',
							'eventLabel': $title.text().trim()
						});
					}
					
				}
			}
        });
    });
})(jQuery);
</script>	</section>


