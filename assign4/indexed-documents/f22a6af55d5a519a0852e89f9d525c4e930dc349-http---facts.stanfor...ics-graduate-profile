<script type="text/javascript">
  window.onload = function () {
    var gender, chartGender
      , school,     chartSchool
      , ethnic, chartEthnic
      , geography, chartGeography
      , degrees, chartDegrees     
      ;
// Gender Balance
    gender = {
        male:   parseFloat($('table#gender td#male').text().trim().replace('%',''))
      , female: parseFloat($('table#gender td#female').text().trim().replace('%',''))
    };
    chartGender = new CanvasJS.Chart("grad-gender-balance-pie", {
      theme: "theme3",
      backgroundColor: "#FBFBF9",
      toolTip:{ enabled: false},
      data: [//array of dataSeries              
        { //dataSeries object

         /*** Change type "column" to "bar", "area", "line" or "pie"***/
         type: "pie",
         indexLabelFontColor: "#000",
         indexLabelFontSize: 14,
         //indexLabelPlacement: "inside",
         dataPoints: [
         { label: "Men",   y: gender.male,   indexLabel: "Men "   + gender.male.toFixed(0)  + "%" },
         { label: "Women", y: gender.female, indexLabel: "Women " + gender.female.toFixed(0) + "%" },
         ]
       }
       ]
     });
    chartGender.render();
    
// Geography Balance
    geography = {
        california:   parseFloat($('table#geography td#california').text().trim().replace('%',''))
      , other: parseFloat($('table#geography td#us-other').text().trim().replace('%',''))
      , international: parseFloat($('table#geography td#international').text().trim().replace('%',''))
    };
    chartGeography = new CanvasJS.Chart("grad-geographic-pie", {
      theme: "theme3",
      backgroundColor: "#FBFBF9",
      toolTip:{ enabled: false},
      data: [//array of dataSeries              
        { //dataSeries object

         /*** Change type "column" to "bar", "area", "line" or "pie"***/
         type: "pie",
         indexLabelFontColor: "#000",
         indexLabelFontSize: 14,
         //indexLabelPlacement: "inside",
         dataPoints: [
         { label: "California",   y: geography.california,   indexLabel: "California "   + geography.california.toFixed(0)  + "%" },
         { label: "Other U.S.", y: geography.other, indexLabel: "Other U.S. " + geography.other.toFixed(0) + "%" },
         { label: "International", y: geography.international, indexLabel: "International " + geography.international.toFixed(0) + "%" },
         ]
       }
       ]
     });
    chartGeography.render();
    

 // School
    school = {
        humsci:  parseFloat($('table#schools td#humsci').text().trim().replace('%',''))
      , eng: parseFloat($('table#schools td#eng').text().trim().replace('%',''))
      , earthsci:    parseFloat($('table#schools td#earthsci').text().trim().replace('%',''))
      , gsb:    parseFloat($('table#schools td#gsb').text().trim().replace('%',''))
      , gse:    parseFloat($('table#schools td#gse').text().trim().replace('%',''))
      , law:    parseFloat($('table#schools td#law').text().trim().replace('%',''))
      , med:    parseFloat($('table#schools td#med').text().trim().replace('%',''))
      , cont:    parseFloat($('table#schools td#cont').text().trim().replace('%',''))
    };
    chartSchool = new CanvasJS.Chart("grad-school-pie", {
      theme: "theme3",
      backgroundColor: "#FBFBF9",
      toolTip:{ enabled: false},
      data: [//array of dataSeries              
        { //dataSeries object

         /*** Change type "column" to "bar", "area", "line" or "pie"***/
         type: "pie",
         indexLabelFontColor: "#000",
         indexLabelFontSize: 14,
         dataPoints: [
         { label: "Humanities & Sciences",        y: school.humsci,  indexLabel: "Humanities & Sciences "        + school.humsci.toFixed(0)  + "%" },
         { label: "Education", y: school.gse,    indexLabel: "Education" + school.gse.toFixed(0)    + "%" },
         { label: "Engineering",       y: school.eng, indexLabel: "Engineering "       + school.eng.toFixed(0) + "%" },
         { label: "Earth, Energy & Environment",   y: school.earthsci,    indexLabel: "Earth, Energy & Environment"   + school.earthsci.toFixed(0)    + "%" },
         { label: "Business", y: school.gsb,    indexLabel: "Business " + school.gsb.toFixed(0)    + "%" },
         { label: "Law", y: school.law,    indexLabel: "Law " + school.law.toFixed(0)    + "%" },
         { label: "Medicine", y: school.med,    indexLabel: "Medicine " + school.med.toFixed(0)    + "%" },
         { label: "Continuing Studies", y: school.cont,    indexLabel: "Continuing Studies " + school.cont.toFixed(0)    + "%" } ,           
         ]
       }
       ]
     });
    chartSchool.render();

  // Ethnic Diversity
    ethnic = {
        afam:     parseFloat($('table#ethnic td#afam').text().trim().replace('%',''))
      , asian:    parseFloat($('table#ethnic td#asian').text().trim().replace('%',''))
      , intl:     parseFloat($('table#ethnic td#intl').text().trim().replace('%',''))
      , native:   parseFloat($('table#ethnic span#native').text().trim().replace('%',''))
      , pacific:  parseFloat($('table#ethnic span#pacific').text().trim().replace('%',''))
      , hispanic: parseFloat($('table#ethnic td#hispanic').text().trim().replace('%',''))
      , white:    parseFloat($('table#ethnic td#white').text().trim().replace('%',''))
      , unknown:  parseFloat($('table#ethnic td#unknown').text().trim().replace('%',''))
      , twoormore:  parseFloat($('table#ethnic td#twoormore').text().trim().replace('%',''))
    };
    chartEthnic = new CanvasJS.Chart("grad-ethnic-diversity-pie", {
      theme: "theme3",
      backgroundColor: "#FBFBF9",
      toolTip:{ enabled: false},
      data: [//array of dataSeries              
        { //dataSeries object

         /*** Change type "column" to "bar", "area", "line" or "pie"***/
         type: "doughnut",
         indexLabelFontColor: "#000",
         indexLabelFontFamily: "Source Sans Pro",
         indexLabelFontSize: 14,
         dataPoints: [
         { label: "Native American",                    y: ethnic.native,   indexLabel: "Native American < "   + ethnic.native.toFixed(0)   + "%" },
         { label: "Native Hawaiian / Pacific Islander", y: ethnic.pacific,  indexLabel: "Native Hawaiian / Pacific Islander < " + ethnic.pacific.toFixed(0) + "%" },
         { label: "Other Hispanic",                     y: ethnic.hispanic, indexLabel: "Other Hispanic "    + ethnic.hispanic.toFixed(0) + "%" },
         { label: "White",                              y: ethnic.white,    indexLabel: "White "             + ethnic.white.toFixed(0)    + "%" },
         { label: "Unknown",                            y: ethnic.unknown,  indexLabel: "Unknown "           + ethnic.unknown.toFixed(0)  + "%" },
         { label: "African American",                   y: ethnic.afam,     indexLabel: "African American "  + ethnic.afam.toFixed(0)     + "%" },
         { label: "Asian",                              y: ethnic.asian,    indexLabel: "Asian "             + ethnic.asian.toFixed(0)    + "%" },
         { label: "International",                      y: ethnic.intl,     indexLabel: "International "     + ethnic.intl.toFixed(0)     + "%" },
         { label: "Two or more races",                  y: ethnic.twoormore,indexLabel: "Two or more races "     + ethnic.twoormore.toFixed(0)     + "%" }           
       ]
       }
       ]
     });
    chartEthnic.render();
    
// Degrees
    degrees = {
        masters:   parseFloat($('table#degrees td#masters').text().trim().replace('%',''))
      , professional: parseFloat($('table#degrees td#prof').text().trim().replace('%',''))
      , doctoral: parseFloat($('table#degrees td#doctoral').text().trim().replace('%',''))
    };
    chartDegrees = new CanvasJS.Chart("grad-degrees-pie", {
      theme: "theme3",
      backgroundColor: "#FBFBF9",
      toolTip:{ enabled: false},
      data: [//array of dataSeries              
        { //dataSeries object

         /*** Change type "column" to "bar", "area", "line" or "pie"***/
         type: "pie",
         indexLabelFontColor: "#000",
         indexLabelFontSize: 14,
         //indexLabelPlacement: "inside",
         dataPoints: [
         { label: "Masters",   y: degrees.masters,   indexLabel: "Masters "   + degrees.masters.toFixed(0)  + "%" },
         { label: "Professional", y: degrees.professional, indexLabel: "Professional " + degrees.professional.toFixed(0) + "%" },
         { label: "Doctoral", y: degrees.doctoral, indexLabel: "Doctoral " + degrees.doctoral.toFixed(0) + "%" },
         ]
       }
       ]
     });
    chartDegrees.render();
        
  }
</script>
