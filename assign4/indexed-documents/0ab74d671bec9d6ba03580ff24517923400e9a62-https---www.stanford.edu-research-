  <main id="main" role="main" tabindex="-1">
    <h2 id="main-content" class="sr-only-element">Main Content</h2>
    <header>
      <h1>Research</h1>

      
    </header><!-- .entry-header -->

    
<section class="panel-collection" data-modular-content-collection="">

	
  <section class="panel" data-type="wysiwyg" data-modular-content="" data-js="panel" data-index="0" data-width="content">

    
<div class="content">
<p style="text-align: center; font-size: 1.5625em;">From Nobel Prize winners to undergraduates, all members of the Stanford community are&nbsp;engaged&nbsp;in creating new knowledge.</p>
</div>


  </section><!-- panel Positioning Statement -->



  <section class="panel" data-type="hero-image" data-modular-content="" data-js="panel" data-index="1" data-width="full">

    
