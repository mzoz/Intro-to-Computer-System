	<section id="panel_61" class="panel panel-count-13 panel-type-mashup panel-even  panel-contained panel-white has-panel-title-inside" data-url-key="slsnav-people" data-nav-title="People" data-panel-type="mashup" data-panel-layout="six-up">
		
